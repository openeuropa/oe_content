declare(strict_types = 1);

/**
 * Implements hook_preprocess_HOOK().
 */
function oe_content_featured_media_field_preprocess_pattern_content_banner(&$variables) {
  if($variables['image']['oe_featured_media']['#image']['src']) {
    $variables['image'] = $variables['image']['oe_featured_media']['#image'];
  }
}

